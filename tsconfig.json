{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true,
		"lib": ["ESNext", "DOM"],
		"types": [
			"vite/client",
			"@cloudflare/workers-types/2023-07-01",
			"@types/bun"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": ".",
		"paths": {
			"@/*": ["app/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"]
}
