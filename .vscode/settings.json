{
	"terminal.integrated.defaultProfile.linux": "bash",
	"git.branchProtection": [
		"main",
	],
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"files.encoding": "utf8",
	"files.eol": "\n",
	"files.insertFinalNewline": true,
	"[markdown]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	},
	"[sql]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	"[json]": {
		"editor.insertSpaces": false
	},
	"[jsonc]": {
		"editor.insertSpaces": false
	},
	"[javascript]": {
		"editor.insertSpaces": false
	},
	"[typescript]": {
		"editor.insertSpaces": false
	},
	"[javascriptreact]": {
		"editor.insertSpaces": false
	},
	"[typescriptreact]": {
		"editor.insertSpaces": false
	}
}
